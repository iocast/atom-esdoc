{
  "name": "atom-esdoc",
  "main": "./lib/esdoc",
  "version": "0.3.0",
  "description": "Atom package for quick esdoc comment generation.",
  "keywords": [
    "esdoc",
    "api",
    "doc"
  ],
  "scripts": {
    "build-doc": "./node_modules/.bin/esdoc"
  },
  "activationCommands": {
    "atom-workspace": [
      "esdoc:generate"
    ],
    "atom-text-editor": [
      "esdoc:parse-enter",
      "esdoc:parse-tab"
    ]
  },
  "repository": "https://github.com/iocast/atom-esdoc",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    }
  },
  "dependencies": {
    "babel-traverse": "^6.25.0",
    "babylon": "^6.17.3"
  },
  "devDependencies": {
    "esdoc": "^1.0.4",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0"
  }
}
